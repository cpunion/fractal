{
  "name": "fractalui",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "start": "node_modules/react-native/packager/packager.sh",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "apphub": "^0.5.0",
    "immutable": "^3.7.6",
    "pro-inputs": "^1.2.6",
    "react": "^0.14.3",
    "react-immutable-proptypes": "^1.5.1",
    "react-native": "^0.22.2",
    "react-native-redux-segment": "^1.0.0",
    "react-redux": "^4.4.0",
    "redux": "^3.3.0",
    "redux-undo": "^0.6.1",
    "reselect": "^2.0.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-jest": "^9.0.3",
    "babel-polyfill": "^6.7.4",
    "babel-preset-react-native": "^1.5.2",
    "babel-register": "^6.7.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.13.1",
    "jest-cli": "^0.9.2",
    "shelljs": "^0.6.0"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "./node_modules/react"
    ],
    "moduleFileExtensions": ["js", "json", "jsx"],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+[.]png$": "RelativeImageStub"
    }
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "react-native"
        ]
      }
    }
  }
}
